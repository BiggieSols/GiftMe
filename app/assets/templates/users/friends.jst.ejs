<h1>Friends</h1>

<h3>You have <%= _("friend").pluralize(friends.models.length, true) %> using GiftMe</h3>
<% friends.models.forEach(function(friend) { %>
  <div class="friend">
    <a href="<%= "/#/users/" + friend.id %>">
      <img src="<%= friend.escape("small_picture_url") %>">
      <%= friend.escape("name") %>
    </a>
  </div>
<% }) %>